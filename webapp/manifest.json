{
	"_version": "1.12.0",
	"sap.app": {
		"id": "glacelx.glacelx",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"fullWidth": true,
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "glacelx.glacelx.view.Intro",
			"type": "XML",
			"async": true,
			"id": "Intro"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "glacelx.glacelx.i18n.i18n"
				}
			},
			"userXML": {
				"type": "sap.ui.model.xml.XMLModel",
				"uri": "./model/example.xml"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "glacelx.glacelx.view",
				"controlAggregation": "pages",
				"controlId": "IntroView",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "intro",
					"pattern": "",
					"target": "Intro"
				},
				{
					"name": "header",
					"pattern": "Header",
					"greedy": false,
					"target": "Header"
				},
				{
					"name": "elements",
					"pattern": "Elements",
					"greedy": false,
					"target": "Elements"
				},
				{
					"name": "systems",
					"pattern": "Systems",
					"greedy": false,
					"target": "Systems"
				},
				{
					"name": "system",
					"pattern": "System/{sysIndex}",
					"greedy": false,
					"target": "System"
				},
				{
					"name": "part",
					"pattern": "Part/{sysIndex}/{partIndex}",
					"greedy": false,
					"target": "Part"
				},
				{
					"name": "results",
					"pattern": "Results/{sysIndex}/{partIndex}",
					"greedy": false,
					"target": "Results"
				}
			],
			"targets": {
				"Intro": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "intro",
					"viewName": "Intro",
					"viewLevel": 1
				},
				"Elements": {
					"viewType": "XML",
					"viewId": "elements",
					"viewName": "Elements",
					"transition": "slide",
					"viewLevel": 2
				},
				"Header": {
					"viewType": "XML",
					"viewId": "header",
					"viewName": "Header",
					"transition": "slide",
					"viewLevel": 3
				},
				"Systems": {
					"viewType": "XML",
					"viewId": "systems",
					"viewName": "Systems",
					"viewLevel": 3,
					"transition": "slide"
				},
				"System": {
					"viewType": "XML",
					"viewId": "System",
					"viewName": "System",
					"viewLevel": 4
				},
				"Part": {
					"viewType": "XML",
					"viewName": "Part",
					"viewId": "part",
					"viewLevel": 5
				},
				"Results": {
					"viewType": "XML",
					"viewName": "Results",
					"viewId": "results",
					"viewLevel": 5
				},
				"Properties": {
					"viewType": "XML",
					"viewName": "Properties"
				}
			}
		}
	}
}